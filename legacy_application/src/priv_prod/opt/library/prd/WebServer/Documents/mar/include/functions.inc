<?phpfunction get_date_from_week($year, $week){global $year, $week_one_start, $week;	$first_day = strtotime($year."-01-01");	$is_monday = date("w", $first_day) == 1;	$is_weekone = strftime("%V", $first_day) == 1;	if($is_weekone)	{		$week_one_start = $is_monday ? $first_day : strtotime("last monday", $first_day);	}	else	{		$week_one_start = strtotime("next monday", $first_day);	}	return $week_one_start+(3600*24*7*($week-1));}/*$year = "2002";$week = "1";get_date_from_week($year, $week);$weekDate = date("M-d-Y", $week_one_start+(3600*24*7*($week-1)-3600*24));//$weekDate = date(M-d-Y, $week_one_start+(3600*24*7*($week-1)));*/function getWeekNumber(){global $year, $week_one_start, $week, $yearRadio, $weekEnd, $weekNumber, $i, $n;$year = "2002"; for ($i=1;$i<=52;$i++){$week = $i;$weekEnd = date("M-d-Y", get_date_from_week($year, $week));//$list = $weekEnd[$i];$weekNumber = array(1=> "Week 1 ends $weekend","Week 2 ends $weekend","Week 3 ends $list[3]","Week 4 ends $list[4]","Week 5 ends $list[4]","Week 6 ends $list[6]","Week 7 ends $list[7]","Week 8 ends $list[8]");return $weekNumber;}}?>